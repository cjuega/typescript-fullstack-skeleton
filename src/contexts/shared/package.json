{
  "name": "@context/shared",
  "version": "1.0.1",
  "description": "Shared bounded context with common utilities",
  "keywords": [],
  "author": "Carlos Juega",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/"
  ],
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./": "./dist/"
  },
  "scripts": {
    "precommit": "npx lint-staged",
    "lint": "npx eslint . --ext .js,.ts",
    "test": "yarn test:unit",
    "test:unit": "NODE_ENV=test jest --verbose --passWithNoTests",
    "build": "yarn build:clean && yarn build:tsc",
    "build:clean": "rm -r dist; exit 0",
    "build:tsc": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist"
  },
  "dependencies": {
    "aws-sdk": "^2.1082.0",
    "aws-xray-sdk": "^3.3.4",
    "dynamodb-onetable": "^2.3.2",
    "faker": "^5.5.3",
    "fast-glob": "^3.2.11",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "uuid": "^8.3.2",
    "uuid-validate": "^0.0.3"
  },
  "devDependencies": {
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.4.1",
    "@types/jest-when": "^3.5.0",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "@types/uuid-validate": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-module-resolver": "^1.4.0",
    "jest": "^27.5.1",
    "jest-when": "^3.5.1",
    "lint-staged": "^12.3.4",
    "ts-jest": "^27.1.3",
    "tscpaths": "^0.0.9",
    "typescript": "^4.5.5",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
}

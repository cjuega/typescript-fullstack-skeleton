{
  "name": "@context/example",
  "version": "1.0.1",
  "description": "Example of a bounded context",
  "author": "Carlos Juega",
  "license": "MIT",
  "files": [
    "./dist/"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "exports": {
    "./*": "./src/*.ts"
  },
  "scripts": {
    "precommit": "npx lint-staged",
    "lint": "npx eslint . --ext .js,.ts",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "NODE_ENV=test jest --verbose",
    "test:integration": "NODE_ENV=test jest --config=jest.config.integration.js --verbose --runInBand --passWithNoTests",
    "build": "yarn build:clean && yarn build:tsc",
    "build:clean": "rm -r dist; exit 0",
    "build:tsc": "tsc --build tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist"
  },
  "dependencies": {
    "@context/shared": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-module-resolver": "^1.4.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "ts-jest": "^29.1.1",
    "tsarch": "^5.3.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.2.2",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
}
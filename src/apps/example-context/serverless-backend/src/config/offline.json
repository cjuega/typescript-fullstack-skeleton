{
    "eventbridge": {
        "region": "eu-west-1",
        "endpoint": "http://localhost:4010",
        "source": "company.service.offline",
        "eventBusName": "example-context-offline-events"
    },
    "dynamodb": {
        "region": "localhost",
        "endpoint": "http://localhost:8000"
    },
    "ddbonetable": {
        "tableName": "example-context-offline-table",
        "indexes": {
            "primary": {
                "hash": "pk",
                "sort": "sk"
            },
            "example-context-offline-gsi1": {
                "hash": "gsi1pk",
                "sort": "gsi1sk"
            }
        },
        "logger": true
    }
}